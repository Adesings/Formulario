  @model Formulario.Models.Alumno

<div class="row">
    <h1> Formulario Ingreso Alumnos</h1>
    <div class="row">
        <div class="form-group">
            <label class="control-label col-sm-2" for="rut">Rut:</label>
            <div class="col-sm-6">
                @Html.DisplayFor(m => m.rut)
            </div>
        </div>
    </div>

    <div class="row">
        <div class="form-group">
            <label class="control-label col-sm-2" for="nombre">Nombre:</label>
            <div class="col-sm-6">
                @Html.DisplayFor(m => m.nombre) @Html.DisplayFor(m => m.apellido)


            </div>
        </div>
    </div>
    <div class="row">
        <div class="form-group">
            <label class="control-label col-sm-2" for="nombre">Fono:</label>
            <div class="col-sm-6">
                @Html.DisplayFor(m => m.fono)


            </div>
        </div>
    </div>
    <div class="row">
        <div class="form-group">
            <label class="control-label col-sm-2" for="apellido">Sexo:</label>
            <div class="col-sm-6">
                @Html.DisplayFor(m => m.sexo)


            </div>
        </div>
    </div>
    <div class="row">
        <div class="form-group">
            <label class="control-label col-sm-2" for="ciudad">Ciudad:</label>
            <div class="col-sm-6">
                @Html.DisplayFor(m => m.ciudad)


            </div>
        </div>
    </div>
</div>